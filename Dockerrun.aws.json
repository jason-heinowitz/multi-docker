{
  "AWSEBDockerrunVersion": "2",
  "containerDefinitions": [
    {
      "name": "client",
      "image": "jheinowitz/multi-client",
      "hostname": "client",
      "essential": false,
      "memory": "128"
    },
    {
      "name": "server",
      "image": "jheinowitz/multi-server",
      "hostname": "api",
      "essential": false,
      "memory": "128"
    },
    {
      "name": "worker",
      "image": "jheinowitz/multi-worker",
      "essential": false,
      "memory": "128"
    },
    {
      "name": "nginx",
      "image": "jheinowitz/multi-nginx",
      "essential": true,
      "memory": "128",
      "portMappings": [
        {
          "hostPort": "80",
          "containerPort": "80"
        }
      ],
      "links": [
        "client",
        "server"
      ]
    }
  ]
}